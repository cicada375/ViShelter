<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:map="http://schemas.android.com/apk/res-auto"


    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/coordinatorLayout">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="393dp"
        android:layout_height="645dp"
        map:layout_anchorGravity="bottom|center"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="1.0"
        map:layout_constraintStart_toStartOf="@id/toolbar"
        tools:context=".MapsActivity"
        tools:layout="@layout/support_simple_spinner_dropdown_item"
        tools:layout_conversion_absoluteHeight="64dp"
        tools:layout_conversion_absoluteWidth="411dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="393dp"
        android:layout_height="140dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/photo_2022_11_01_15_48_57"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        map:layout_constraintBottom_toTopOf="@+id/map"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="96dp"
        android:layout_height="61dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="265dp"
        android:layout_marginBottom="40dp"
        android:text="@string/app_name"
        map:layout_constraintBottom_toBottomOf="@+id/toolbar"
        map:layout_constraintEnd_toEndOf="@+id/toolbar"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toStartOf="@+id/toolbar"
        map:layout_constraintTop_toTopOf="@+id/toolbar" />

    <Button
        android:id="@+id/button2"
        android:layout_width="96dp"
        android:layout_height="61dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="40dp"
        android:text="ViShelter 2"
        map:layout_constraintBottom_toBottomOf="@+id/toolbar"
        map:layout_constraintEnd_toEndOf="@+id/toolbar"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toStartOf="@+id/toolbar"
        map:layout_constraintTop_toTopOf="@+id/toolbar" />

    <Button
        android:id="@+id/button3"
        android:layout_width="96dp"
        android:layout_height="61dp"
        android:layout_marginStart="268dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="40dp"
        android:text="ViShelter 2"
        map:layout_constraintBottom_toBottomOf="@+id/toolbar"
        map:layout_constraintEnd_toEndOf="@+id/toolbar"
        map:layout_constraintHorizontal_bias="1.0"
        map:layout_constraintStart_toStartOf="@+id/toolbar"
        map:layout_constraintTop_toTopOf="@+id/toolbar" />


</androidx.constraintlayout.widget.ConstraintLayout>